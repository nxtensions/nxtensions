{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "NxtensionsAstroPresetGenerator",
  "title": "Astro Preset Generator",
  "type": "object",
  "properties": {
    "astroAppName": {
      "type": "string",
      "description": "Name of the initial application to be generated.",
      "x-prompt": "Application name"
    },
    "tags": {
      "type": "string",
      "description": "Add tags to the project (used for linting).",
      "alias": "t"
    }
  },
  "required": ["astroAppName"]
}
